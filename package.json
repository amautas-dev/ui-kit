{
  "name": ".",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:ui-kit": "ng-packagr -p projects/ui-kit/ng-package.json",
    "generate:ui-kit-tests": "node projects/ui-kit/scripts/generate-test-index.js",
